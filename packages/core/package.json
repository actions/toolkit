{
  "name": "@unlike/github-actions-core",
  "version": "0.0.4",
  "description": "Actions core lib",
  "keywords": [
    "github",
    "actions",
    "core"
  ],
  "homepage": "https://github.com/unlike-ltd/github-actions-toolkit/tree/main/packages/core",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "types": "./dist/types/index.d.ts",
  "exports": {
    "node": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlike-ltd/github-actions-toolkit.git",
    "directory": "packages/core"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "build": "rm -rf dist && tsup-node && pnpm run build:types",
    "build:types": "tsc -p tsconfig.declarations.json",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "bugs": {
    "url": "https://github.com/unlike-ltd/github-actions-toolkit/issues"
  },
  "engines": {
    "node": ">=v16.20.0"
  },
  "dependencies": {
    "@actions/http-client": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "16.18.36",
    "@types/uuid": "^9.0.2",
    "tsconfig": "workspace:*",
    "tsup": "7.0.0",
    "typescript": "5.1.3"
  }
}

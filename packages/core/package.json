{
  "name": "@actions/core",
  "version": "1.10.0",
  "description": "Actions core lib",
  "keywords": [
    "github",
    "actions",
    "core"
  ],
  "homepage": "https://github.com/actions/toolkit/tree/main/packages/core",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "types": "./lib/types/core.d.ts",
  "exports": {
    "node": {
      "types": "./lib/types/core.d.ts",
      "import": "./lib/esm/core.js",
      "require": "./lib/cjs/core.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/toolkit.git",
    "directory": "packages/core"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "build": "rm -rf lib && tsup-node && pnpm run build:types",
    "build:types": "tsc -p tsconfig.declarations.json",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf lib"
  },
  "bugs": {
    "url": "https://github.com/actions/toolkit/issues"
  },
  "dependencies": {
    "@actions/http-client": "^2.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "16.18.36",
    "@types/uuid": "^9.0.2",
    "tsconfig": "workspace:*",
    "tsup": "6.7.0",
    "typescript": "5.1.3"
  }
}
